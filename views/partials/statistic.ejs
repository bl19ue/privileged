                                            <!--options="bar.options"></canvas>-->
                                <!--</div>-->
                            <!--</div>-->
                        <!--</div>-->
                    </div>

                    <!-- Charts code -->

                </div>
            </div>
        </div>
    </div>

    <aside class="control-sidebar">
        <highchart id="chart1" config="statsVm.expertiseChart"></highchart>
    </aside>
</div>